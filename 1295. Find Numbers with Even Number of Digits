class Solution {
    public int findNumbers(int[] nums) {
        
        int total=0;
        for(int n:nums){
            int digit=0;
            while(n!=0){
                n=n/10;
                digit++;
            }
            
         if(digit%2==0){
                total++;
            }
            
            
        }
    
        return total;
        
    }
}
